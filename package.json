{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "npm-run-all --parallel --print-label --race dev-*",
    "dev-remix": "remix dev",
    "dev-styles": "postcss styles --base styles --dir app/styles -w",
    "build": "pnpm build-styles && remix build",
    "build-styles": "postcss styles --base styles --dir app/styles",
    "prepare": "remix setup node && pnpm build-styles",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "0.20.1",
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "10.4.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "postcss-cli": "9.0.2",
    "tailwindcss": "3.0.0-alpha.2",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
